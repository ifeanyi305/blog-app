<% @users.each do |user| %>
  <div>
    <%= link_to "Sign out", destroy_user_session_path, method: :delete %>
    <div><img src="<%= user.photo %>" alt="User" /></div>
    <div>
      <h1><%= link_to user.name, user %></h1>
      <span>number of post: <%= user.posts.count %></span>
    </div>
  </div>
  <%= link_to "view", user %>
<% end %>
